    <IsPackable>true</IsPackable>
    <AssemblyName>Microsoft.Liftr.Logging.StaticLogger</AssemblyName>
    <RootNamespace>Microsoft.Liftr.Logging.StaticLogger</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Serilog.Sinks.ApplicationInsights" Version="$(PackageVersion_Serilog_Sinks_ApplicationInsights)" />
