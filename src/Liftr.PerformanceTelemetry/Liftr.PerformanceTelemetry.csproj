<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>$(LiftrExeTargetFramework)</TargetFramework>
    <IsPackable>true</IsPackable>
    <AssemblyName>Microsoft.Liftr.PerformanceTelemetry</AssemblyName>
    <RootNamespace>Microsoft.Liftr.PerformanceTelemetry</RootNamespace>
    <DisableLiftrCodeAnalysis>true</DisableLiftrCodeAnalysis>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Caching.Abstractions" Version="$(PackageVersion_Microsoft_Extensions_Caching_Abstractions)" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Liftr.Logging\Liftr.Logging.csproj" />
  </ItemGroup>
</Project>