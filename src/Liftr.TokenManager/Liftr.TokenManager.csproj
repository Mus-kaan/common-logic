<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>$(LiftrClassLibraryTargetFramework)</TargetFramework>
    <IsPackable>true</IsPackable>
    <AssemblyName>Microsoft.Liftr.TokenManager</AssemblyName>
    <RootNamespace>Microsoft.Liftr.TokenManager</RootNamespace>
    <Description>Provides common infra for Token Manage, for AAD</Description>
    <PackageProjectUrl>https://msazure.visualstudio.com/Liftr/_git/Liftr.Common?path=%2Fsrc%2FLiftr.TokenManager%2FReadMe.md&amp;version=GBmaster</PackageProjectUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.IdentityModel.Clients.ActiveDirectory" Version="$(PackageVersion_Microsoft_IdentityModel_Clients_ActiveDirectory)" />
    <PackageReference Include="Microsoft.Azure.KeyVault" Version="$(PackageVersion_Microsoft_Azure_KeyVault)" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Liftr.AAD\Liftr.AAD.csproj" />
    <ProjectReference Include="..\Liftr.Logging\Liftr.Logging.csproj" />
  </ItemGroup>
</Project>