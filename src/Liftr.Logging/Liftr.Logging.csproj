<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>$(LiftrClassLibraryTargetFramework)</TargetFramework>
    <IsPackable>true</IsPackable>
    <AssemblyName>Microsoft.Liftr.Logging</AssemblyName>
    <RootNamespace>Microsoft.Liftr.Logging</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Serilog" Version="$(PackageVersion_Serilog)" />
    <PackageReference Include="Serilog.Formatting.Compact" Version="$(PackageVersion_Serilog_Formatting_Compact)" />
    <PackageReference Include="Serilog.Sinks.Console" Version="$(PackageVersion_Serilog_Sinks_Console)" />
    <PackageReference Include="Serilog.Sinks.File" Version="$(PackageVersion_Serilog_Sinks_File)" />
  </ItemGroup>
</Project>