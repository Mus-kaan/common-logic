{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "%TEMP%\\Logs\\SampleWebApp\\serilog-configuration-sample.txt"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "Application": "SampleConsoleApp"
    }
    //"AllowFilterDynamicOverride": false
  },
  "ApplicationInsights": {
    "InstrumentationKey": "67a68a43-9f53-4729-8bd6-4271884950d6" // ibuild-dev
  },
  "AllowedHosts": "*"
  //"VaultEndpoint": "https://ibuild-rpdata-cus.vault.azure.net/"
}
