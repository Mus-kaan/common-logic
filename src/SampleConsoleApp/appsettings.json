{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      //{
      //  "Name": "Console"
      //},
      {
        "Name": "File",
        "Args": {
          "path": "%TEMP%\\Logs\\SampleConsoleApp\\log.txt",
          "rollingInterval": "Day",
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "Application": "SampleConsoleApp"
    }
    //"AllowFilterDynamicOverride": false
  },
  "ApplicationInsights": {
    "InstrumentationKey": "b31bef3a-fd24-4953-aec6-310c7bf48c43" // 'liftr-dev-wus2-appinsights' in 'Liftr - TEST' subscription
  },
  "AllowedHosts": "*"
    
  //"VaultEndpoint": "https://ibuild-rpdata-cus.vault.azure.net/"
}
