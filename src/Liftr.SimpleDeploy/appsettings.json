{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "%TEMP%\\Logs\\SampleWebApp\\serilog-configuration-sample.txt"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "Application": "provisioning-runner"
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": "14b26785-c70d-4a54-8656-9da973cb21b0" // 'liftr-ev2-prod-ai' in 'Liftr - TEST'
  },
  "AllowedHosts": "*",
  "EnvironmentOptions": {
    "GenevaCert": {
      "CertificateName": "GenevaAMEClientCert",
      "SubjectName": "antares-mds-agent.geneva.keyvault.liftr-dev.net",
      "SubjectAlternativeNames": [ "antares-mds-agent.geneva.keyvault.liftr-dev.net" ]
    },
    "AKSInfo": {
      "AKSRootUserName": "liftraksvmuser",
      "AKSSSHPublicKey": "ssh-rsa  weng5e@5e-box",
      "AKSSPNClientSecretName": "AKSSPClientSecret",
      "AKSMachineCount": 3,
      "AKSMachineTypeStr": "Standard_DS2_v2"
    }
  }
}
