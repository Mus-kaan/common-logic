<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>$(LiftrClassLibraryTargetFramework)</TargetFramework>
    <IsPackable>true</IsPackable>
    <AssemblyName>Microsoft.Liftr.Logging.Metrics</AssemblyName>
    <RootNamespace>Microsoft.Liftr.Logging.Metrics</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
