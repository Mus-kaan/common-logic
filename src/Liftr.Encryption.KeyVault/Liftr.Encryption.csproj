<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>$(LiftrClassLibraryTargetFramework)</TargetFramework>
    <IsPackable>true</IsPackable>
    <AssemblyName>Microsoft.Liftr.Encryption</AssemblyName>
    <RootNamespace>Microsoft.Liftr.Encryption</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.KeyVault" Version="$(PackageVersion_Microsoft_Azure_KeyVault)" />
    <PackageReference Include="Microsoft.Azure.KeyVault.Extensions" Version="$(PackageVersion_Microsoft_Azure_KeyVault_Extensions)" />
    <PackageReference Include="Microsoft.Azure.Services.AppAuthentication" Version="$(PackageVersion_Microsoft_Azure_Services_AppAuthentication)" />
    <PackageReference Include="Microsoft.IdentityModel.Clients.ActiveDirectory" Version="$(PackageVersion_Microsoft_IdentityModel_Clients_ActiveDirectory)" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Liftr.Contracts\Liftr.Contracts.csproj" />
    <ProjectReference Include="..\Liftr.Encryption.Abstractions\Liftr.Encryption.Abstractions.csproj" />
    <ProjectReference Include="..\Liftr.Logging\Liftr.Logging.csproj" />
  </ItemGroup>
</Project>